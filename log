  open.node; if [ $go -eq 1 ]; then
    3dZeropad \
    -RL "$gRL" \
    -AP "$gAP" \
    -IS "$gIS" \
    -prefix ${out[$i]} \
    ${in[$i]} &> ${prefix[$i]}$i.log 
  fi; close.node
  log "HOMOGENIZE RS ";
  toRS
done
