    3dvolreg \
    -prefix ${out[$i]} \
    -base 100 \
    -zpad 2 \
    -twopass \
    -Fourier \
    -verbose \
    -1Dfile ${out_2[$i]} \
    ${in[$i]} &>> ${prefix[$i]}$i.log && \
    1dplot \
    -jpg ${out_3[$i]} \
    -volreg -dx $TR \
    -xlabel Time \
    -thick \
    ${out_2[$i]} &>> ${prefix[$i]}$i.log 
  fi; close.node
  log "Motion Correction "
done
